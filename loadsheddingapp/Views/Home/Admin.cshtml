@{
  ViewData["Title"] = "Admin Page";
}



<form class="row g-3 needs-validation" id="admin" method="post" action="/" novalidate>
  <div class="col-sm-12">
    <label for="joke" class="form-label">the joke the joke the joke the joke the joke the joke the joke the joke the joke the joke the joke the joke the joke the joke the joke </label>
  </div>
  <div class="col-sm-12">
    <div class="row">
      <div class="col-6">
        <div class="form-check">
          <input class="form-check-input" type="radio" name="flexRadioDefault" id="Accepted">
          <label class="form-check-label" for="Accepted" style="margin: 0px 0px 0px 5px">
            Reject joke
          </label>
        </div>
      </div>
      <div class="col-6">
        <div class="form-check">
          <input class="form-check-input" type="radio" name="flexRadioDefault" id="Rejected" checked>
          <label class="form-check-label" for="Rejected" style="margin:0px 0px 0px 5px">
            Accept joke
          </label>
        </div>
      </div>
      <input type="hidden" value="" name="jokeid" id="jokeid" />
    </div>
  </div>
  <input name="__RequestVerificationToken" type="hidden" value="@Html.AntiForgeryToken()">
  <button type="submit" class="btn btn-primary">Submit</button>
</form>